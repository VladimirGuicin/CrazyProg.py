# CrazyProg.py

def sort(num):
    for a in range (6):
        pozmin = a
        for b in range(a,7):
            if num[b]< num[pozmin]:
                pozmin=b

        temp=num[a]
        num[a]=num[pozmin]
        num[pozmin]=temp

num=[5, 12, 7, 32, 1, 0, -4]
sort(num)
print(num)

#job is done
